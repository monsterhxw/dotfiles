{
  "alwaysThinkingEnabled": true,
  "attribution": {
    "commit": "",
    "pr": ""
  },
  "env": {
    "ANTHROPIC_AUTH_TOKEN": "",
    "ANTHROPIC_BASE_URL": "",
    "CLAUDE_CODE_DISABLE_NONESSENTIAL_TRAFFIC": "1",
    "CLAUDE_CODE_DISABLE_TERMINAL_TITLE": "1"
  },
  "model": "opus",
  "permissions": {
    "defaultMode": "plan",
    "deny": [
      "Read(~/.ssh/**)",
      "Read(~/.env)",
      "Read(./.env*)",
      "Read(/**/.env*)"
    ]
  },
  "sandbox": {
    "autoAllowBashIfSandboxed": true,
    "enabled": true,
    "excludedCommands": [
      "git",
      "docker"
    ],
    "network": {
      "allowLocalBinding": true
    }
  }
}
